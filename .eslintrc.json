{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn"
    }
}
